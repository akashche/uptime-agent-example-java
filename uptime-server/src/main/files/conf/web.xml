<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
                             http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         id="com.alexkasko.thermostat.uptime.server" version="2.5">

    <display-name>Uptime Agent Web Application</display-name>

    <servlet>
        <servlet-name>uptimeServlet</servlet-name>
        <servlet-class>com.alexkasko.thermostat.uptime.server.UptimeServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>uptimeServlet</servlet-name>
        <url-pattern>/*</url-pattern>
    </servlet-mapping>
    
</web-app>